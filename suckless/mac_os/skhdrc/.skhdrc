# Job apps
alt + shift - b : open /Applications/Arc.app
alt + shift - c : open /Applications/Microsoft\ Teams\ \(work\ or\ school\).app
alt + shift - d : open /Applications/TablePlus.app
alt + shift - f : open /Applications/Messenger.app
alt + shift - g : open /Applications/GitButler.app
alt + shift - i : open /Applications/Burp\ Suite\ Community\ Edition.app
alt + shift - m : open /Applications/Microsoft\ Outlook.app
alt + shift - n : open /Applications/Inkdrop.app
alt + shift - q : open /Applications/qutebrowser.app
alt + shift - s : open /Applications/Slack.app
alt + shift - t : open /Applications/Alacritty.app
alt + shift - w : open /Applications/wpsoffice.app
alt + shift - v : open /Applications/Visual\ Studio\ Code.app

alt - tab : yabai -m space --focus recent
cmd - tab : yabai -m window --focus stack.recent

shift + alt - space : yabai -m window --toggle float

# I dont think i need more than 3 screens at the moment
alt - 1 :yabai -m space --focus 1
alt - 2 :yabai -m space --focus 2
alt - 3 :yabai -m space --focus 3
alt - 4 :yabai -m space --focus 4
alt - 5 :yabai -m space --focus 5
alt - 6 :yabai -m space --focus 6
alt + shift - 1: yabai -m window --space 1; yabai -m space --focus 1
alt + shift - 2: yabai -m window --space 2; yabai -m space --focus 2
alt + shift - 3: yabai -m window --space 3; yabai -m space --focus 3
alt + shift - 4: yabai -m window --space 4; yabai -m space --focus 4
alt + shift - 5: yabai -m window --space 5; yabai -m space --focus 5
alt + shift - 6: yabai -m window --space 6; yabai -m space --focus 6

# Zoom
alt - f         : yabai -m window --toggle zoom-fullscreen

# Moving windows
alt + shift - h : yabai -m window --warp west
alt + shift - j : yabai -m window --warp south
alt + shift - k : yabai -m window --warp north
alt + shift - l : yabai -m window --warp east

# Focus windows
alt - h : yabai -m window --focus west
alt - j : yabai -m window --focus south
alt - k : yabai -m window --focus north
alt - l : yabai -m window --focus east

# Don't commit this
ctrl + alt + shift - y : yabai --restart-service; echo "xxxx" | sudo -S yabai --load-sa; osascript -e 'display notification "Restarted Yabai"'
