# alt - 2 : yabai -m window --focus $(yabai -m query --windows | jq '.[] | select(.["stack-index"] == 2) | .id')
# alt - 3 : yabai -m window --focus $(yabai -m query --windows | jq '.[] | select(.["stack-index"] == 3) | .id')
# alt - 4 : yabai -m window --focus $(yabai -m query --windows | jq '.[] | select(.["stack-index"] == 3) | .id')
# alt - 5 : yabai -m window --focus $(yabai -m query --windows | jq '.[] | select(.["stack-index"] == 3) | .id')

alt - q : open /Applications/DataGrip.app
alt - w : open /Applications/Kitty.app
alt - e : open /Applications/Arc.app
alt - r : open /Applications/Discord.app
alt - t : open /Applications/Slack.app


ctrl + alt + shift - y : yabai --resetart-service
