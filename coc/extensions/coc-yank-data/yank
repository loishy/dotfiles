	                      gift?.purchaseTransaction?.braintreeTransactions[0]?.paymentInstrumentType
	                    if (paymentType) return startCase(paymentType?.split("_")?.join(" "))
	                    return null
	                  })()}
	                />
	              </View>
	              <View style={styles.item}>
