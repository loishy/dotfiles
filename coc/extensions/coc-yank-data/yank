	  const formRef = React.useRef()
ceb9b8d1b0d3af1a8ce1e373c67a270c|file:///Users/louis/Dev/Projects/komi/komi-mobile/app/screens/register-info-screen/register-info-screen.tsx|107|1|V|typescriptreact
	        <Formik
	          innerRef={formRef}
	          initialValues={{
	            firstName: firstName,
	            lastName: lastName,
	            dateOfBirth: undefined,
	            country: undefined,
	            countryName: "",
	            callingCode: "",
	            gender: GENDER.Male,
	            phone: "",
	            email: email,
	          }}
	          onSubmit={_handleSubmit}
	          validationSchema={yup.object().shape({
	            firstName: yup.string().required(translate("errors.fieldRequired")),
	            lastName: yup.string().required(translate("errors.fieldRequired")),
	            dateOfBirth: yup
	              .string()
	              .required(translate("errors.fieldRequired"))
	              .test("ageNotOver16", translate("errors.registerAgeInvalid"), (value) => {
	                return moment(value).diff(getMaxiumAgeDate(16)) < 0
	              }),
	            country: yup.string().required(translate("errors.fieldRequired")),
	            gender: yup.string().required(translate("errors.fieldRequired")),
	            phone: yup
	              .string()
	              .required(translate("errors.fieldRequired"))
	              .test("invalidPhoneNumbers", translate("errors.invalidPhoneNumbers"), (value) => {
	                return value?.match(/\d/g)?.join("").length >= 8
	              }),
	            email: isRegisterWithSocial
	              ? yup.string().email().required(translate("errors.requiredEmail"))
	              : yup.string(),
	          })}
	        >
	          {({ values, handleChange, setFieldValue, touched, handleSubmit, errors }) => {
	            return (
	              <>
	                <TextInput
	                  value={values.firstName}
	                  style={MB_4}
	                  placeholderTx="registerScreen.firstName"
	                  onChangeText={handleChange("firstName")}
	                  errorMessage={touched.firstName && (errors?.firstName as string)}
	                />
	                <TextInput
	                  value={values.lastName}
	                  style={MB_4}
	                  placeholderTx="registerScreen.lastName"
	                  onChangeText={handleChange("lastName")}
	                  errorMessage={touched.lastName && (errors?.lastName as string)}
	                />
	
	                {isRegisterWithSocial && (
	                  <TextInput
	                    editable={!email}
	                    style={MB_4}
	                    placeholderTx="common.emailAddress"
	                    autoCapitalize="none"
	                    keyboardType="email-address"
	                    autoCompleteType="off"
	                    value={values.email}
	                    onChangeText={handleChange("email")}
	                    errorMessage={touched.email && (errors?.email as string)}
	                    inputWrapStyle={!!email && { backgroundColor: color.themeColor.line }}
	                  />
	                )}
	                <TextInputDatePicker
	                  readonly
	                  value={values.dateOfBirth}
	                  style={MB_4}
	                  placeholderTx="registerScreen.dateOfBirth"
	                  rightIcon="calendar"
	                  onChange={(selected) => setFieldValue("dateOfBirth", selected)}
	                  errorMessage={touched.dateOfBirth && (errors?.dateOfBirth as string)}
	                />
	                <TouchableOpacity onPress={_onPressCountry}>
	                  <View pointerEvents="none">
	                    <TextInput
	                      readonly
	                      style={MB_4}
	                      placeholderTx="registerScreen.country"
	                      rightIcon="arrow-down"
	                      onRightIconPress={_onPressCountry}
	                      value={
	                        values.callingCode ? `${values.countryName} (+${values.callingCode}) ` : ""
	                      }
	                      errorMessage={touched.country && (errors?.country as string)}
	                      inputWrapStyle={{
	                        borderWidth: 1,
	                        borderColor: color.line,
	                      }}
	                    />
	                  </View>
	                </TouchableOpacity>
	                <TextInput
	                  value={values.phone}
	                  maxLength={15}
	                  style={MB_4}
	                  keyboardType="phone-pad"
	                  placeholderTx="registerScreen.phoneNumber"
	                  onChangeText={(value) => {
	                    const testNumber = new AsYouType(
	                      selectCountry?.abbreviation || ("GB" as any),
	                    ).input(selectCountry?.abbreviation === "GB" ? `+44 ${value}` : value)
	
	                    setFieldValue(
	                      "phone",
	                      selectCountry?.abbreviation === "GB"
	                        ? testNumber === "+44"
	                          ? ""
	                          : testNumber.replace("+44 ", "")
	                        : testNumber,
	                    )
	                  }}
	                  errorMessage={touched.phone && (errors?.phone as string)}
	                />
	                <View style={MB_4}>
	                  <Text
	                    tx="registerScreen.gender"
	                    preset="regularR"
	                    style={{ color: rgbColor(color.themeColor.dark, 0.7) }}
	                  />
	                  <View style={[MT_4, MB_4, styles.genderWrapper]}>
	                    <Button
	                      style={[
	                        styles.buttonGender,
	                        {
	                          borderColor:
	                            values.gender === GENDER.Male ? color.themeColor.primary : color.line,
	                        },
	                      ]}
	                      onPress={() => handleChange("gender")(GENDER.Male)}
	                    >
	                      <Text preset="regularR" tx="registerScreen.male" />
	                    </Button>
	                    <Button
	                      style={[
	                        styles.buttonGender,
	                        {
	                          borderColor:
	                            values.gender === GENDER.Female ? color.themeColor.primary : color.line,
	                        },
	                      ]}
	                      onPress={() => handleChange("gender")(GENDER.Female)}
	                    >
	                      <Text preset="regularR" tx="registerScreen.female" />
	                    </Button>
	                    <Button
	                      style={[
	                        styles.buttonGender,
	                        {
	                          borderColor:
	                            values.gender === GENDER.Other ? color.themeColor.primary : color.line,
	                        },
	                      ]}
	                      onPress={() => handleChange("gender")(GENDER.Other)}
	                    >
	                      <Text preset="regularR" tx="registerScreen.other" />
	                    </Button>
	                  </View>
	                </View>
	
	                <Button
	                  disabled={isLoading}
	                  containerStyle={styles.button}
	                  testID="login-button"
	                  tx="common.continue"
	                  onPress={handleSubmit}
	                  loading={isLoading}
	                />
	                <View style={{ height: spacing[7] }} />
	              </>
	            )
