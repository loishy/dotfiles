50bfc61d38af3305253f76e75114cbde|file:///Users/louis/Dev/Projects/komi/komi-mobile/app/screens/purchase-history-screen/consumer-purchase-history-screen.tsx|1|1|V|typescriptreact
	import { useFocusEffect, useNavigation } from "@react-navigation/native"
	import { toJS } from "mobx"
	import { observer } from "mobx-react-lite"
	import { useStores } from "models"
	import { BookingModelType, ContentModelType } from "models/model-types"
	import moment from "moment"
	import { NavigationRoutes } from "navigation/navigation-routes"
	import * as React from "react"
	import { RefreshControl, SectionList, StyleSheet, View } from "react-native"
	import { rgbColor } from "utils/color"
	import { getFirstImage } from "utils/general"
	import { ListEmpty, PlaceHolderList, PLACE_HOLDER_TYPE, Screen, Text } from "../../components"
	import { BOOKING_STATUS } from "../../constants"
	import { BG_WHITE, color, MB_4, MB_5, metrics, PX_4, PY_6, spacing } from "../../theme"
	import { PurchaseHistoryCard } from "./components/purchase-history-card"
	import { useLocalCurrencyContext } from "hooks"
	
	interface ConsumerPurchaseHistoryScreenProps {
	  canFetch: boolean
	}
	
	export const ConsumerPurchaseHistoryScreen = observer(
	  ({ canFetch }: ConsumerPurchaseHistoryScreenProps) => {
	    const {
	      bookingStore: {
	        purchaseHistory: {
	          total,
	          list,
	          isRefreshing,
	          isLoadingMore,
	          getMore,
	          isLoading,
	          getList,
	          refreshList,
	        },
	      },
	    } = useStores()
	    const { formatterCurrency } = useLocalCurrencyContext()
	    const [sections, setSections] = React.useState([])
	    const navigation = useNavigation()
	
	    const _renderSectionHeader = ({ section }) => {
	      return (
	        <Text
	          style={[PX_4, MB_4, BG_WHITE, styles.sectionHeaderTitle]}
	          preset="regularR"
	          text={section.title}
	        />
	      )
	    }
	    const _renderSectionItem = ({ item }) => {
	      return (
	        <PurchaseHistoryCard
	          price={formatterCurrency(item?.price)}
	          priceStyle={{ color: color.themeColor.dark }}
	          fontSizePrice={16}
	          name={item?.experience?.name}
	          talentName={`${item?.talent?.talentProfile?.firstName} ${item?.talent?.talentProfile?.lastName}`}
	          avatar={item?.talent?.talentProfile?.avatar}
	          cover={getFirstImage(item?.experience as ContentModelType)}
	          title={item?.id}
	          status={item?.status as BOOKING_STATUS}
	          style={MB_5}
	          onPress={() =>
	            navigation.navigate(NavigationRoutes.ConsumerPurchaseDetailScreen, {
	              bookingId: item.id,
	            })
	          }
	        />
	      )
	    }
	
	    const formatSection = React.useCallback(
	      (bookings: BookingModelType[]) => {
	        const sectionList = toJS(bookings).reduce((section, booking) => {
	          const key = moment(booking?.createdAt).dayOfYear()
	
	          if (!section[key])
	            section[key] = {
	              key,
	              title: moment(booking?.createdAt).format("MMM DD"),
	              data: [booking],
	            }
	          else section[key].data.push(booking)
	          return section
	        }, {})
	
	        return Object.keys(sectionList)
	          .map((key) => {
	            return sectionList[key]
	          })
	          .reverse()
	      },
	      [list],
	    )
	
	    const _onEndReachedCategories = React.useCallback(() => {
	      const isContinue = list.length < total
	      if (!isLoadingMore && !isRefreshing && isContinue) {
	        getMore()
	      }
	    }, [isRefreshing, isLoadingMore, total, list])
	
	    useFocusEffect(
	      React.useCallback(() => {
	        canFetch &&
	          getList({
	            page: 1,
	            sort: "-createdAt",
	          })
	      }, [canFetch]),
	    )
	
	    React.useEffect(() => {
	      setSections(formatSection(list))
	    }, [list.length])
	
	    return (
	      <View style={styles.container}>
	        {isLoading && !isLoadingMore ? (
	          <PlaceHolderList
	            horizontal={false}
	            elementStyle={[
	              {
	                height: metrics.screenHeight * 0.18,
	                borderRadius: metrics.borderRadius,
	              },
	              MB_5,
	            ]}
	            style={PY_6}
	            type={PLACE_HOLDER_TYPE.PURCHASE_HISTORY_LIST}
	            total={3}
	          />
	        ) : (
	          <SectionList
	            sections={sections}
	            stickySectionHeadersEnabled
	            renderSectionHeader={_renderSectionHeader}
	            ListEmptyComponent={<ListEmpty tx="common.noItem" />}
	            renderItem={_renderSectionItem}
	            showsVerticalScrollIndicator={false}
	            keyExtractor={(item) => item.id}
	            onEndReachedThreshold={0.6}
	            onTouchEndCapture={_onEndReachedCategories}
	            refreshControl={
	              <RefreshControl
	                refreshing={isRefreshing}
	                onRefresh={() => refreshList()}
	                tintColor={color.themeColor.primary}
	              />
	            }
	          />
	        )}
	      </View>
	    )
	  },
	)
	
	const styles = StyleSheet.create({
	  container: {
	    flex: 1,
	    backgroundColor: color.background,
	    padding: spacing[4],
	    justifyContent: "center",
	  },
	  sectionHeaderTitle: {
	    color: rgbColor(color.themeColor.dark, 0.7),
	  },
	})
2675154d50bc54b2cadb10940cd5c645|file:///Users/louis/Dev/Projects/komi/komi-mobile/app/screens/purchase-history-screen/components/purchase-history-card.tsx|1|1|V|typescriptreact
	import * as React from "react"
	import { View, StyleSheet, TouchableOpacity, ViewStyle } from "react-native"
	import { FastImage } from "components"
	import { color, spacing, MR_2, metrics, MB_1 } from "theme"
	import { Divider } from "react-native-elements"
	import { Text, Avatar } from "components"
	import { rgbColor } from "utils/color"
	import { startCase } from "utils/lodash"
	import { BOOKING_STATUS } from "../../../constants"
	
	interface PurchaseHistoryCardProps {
	  style?: ViewStyle
	  cover?: string
	  avatar?: string
	  name?: string
	  talentName?: string
	  title?: string
	  status?: BOOKING_STATUS
	  price?: string
	  priceStyle?: { color: string; fontSize?: number }
	  fontSizePrice?: number
	  onPress?: () => void
	}
	
	export const PurchaseHistoryCard = ({
	  style,
	  cover,
	  title,
	  name,
	  talentName,
	  status,
	  price,
	  priceStyle,
	  fontSizePrice,
	  avatar,
	  onPress,
	}: PurchaseHistoryCardProps) => {
	  const _statusStyle = (status: BOOKING_STATUS) => {
	    let result = {}
	
	    switch (status) {
	      case BOOKING_STATUS.ACCEPTED:
	      case BOOKING_STATUS.COMPLETED:
	        result = styles.successStatus
	        break
	      case BOOKING_STATUS.UNCOMPLETED:
	      case BOOKING_STATUS.DECLINED:
	        result = styles.rejectStatus
	        break
	      case BOOKING_STATUS.PENDING:
	        result = styles.pendingStatus
	        break
	    }
	
	    return result
	  }
	  return (
	    <TouchableOpacity onPress={onPress} style={[styles.container, style]}>
	      <View style={styles.coverContainer}>
	        <FastImage style={styles.cover} source={{ uri: cover }} />
	      </View>
	      <View style={styles.content}>
	        <View style={styles.purchaseInfo}>
	          <View style={styles.purchaseStatus}>
	            <Text
	              preset="regularS"
	              tx="common.purchaseID"
	              txOptions={{ number: title }}
	              style={[MB_1, styles.title]}
	            />
	            {status && (
	              <Text preset="regularS" style={_statusStyle(status)} text={startCase(status)} />
	            )}
	          </View>
	          <Text preset="semiBoldS" style={[priceStyle, { fontSize: fontSizePrice }]} text={price} />
	        </View>
	        <Divider style={styles.divider} />
	        <Text numberOfLines={1} ellipsizeMode="tail" text={name} preset="semiBoldR" style={MB_1} />
	        <View style={styles.info}>
	          <Avatar style={MR_2} source={{ uri: avatar }} title={talentName} />
	          <Text text={talentName} preset="mediumS" />
	        </View>
	      </View>
	    </TouchableOpacity>
	  )
	}
	
	const COVER_WIDTH = metrics.screenWidth * 0.213
	const COVER_HEIGH = metrics.screenHeight * 0.135
	
	const styles = StyleSheet.create({
	  container: {
	    flexDirection: "row",
	    alignItems: "center",
	    padding: spacing[4],
	    borderWidth: 1,
	    borderRadius: metrics.borderRadius,
	    borderColor: rgbColor(color.line, 0.7),
	  },
	  content: {
	    flex: 1,
	    justifyContent: "center",
	    flexDirection: "column",
	  },
	  purchaseInfo: {
	    flexDirection: "row",
	    alignItems: "flex-start",
	    justifyContent: "space-between",
	  },
	  title: {
	    color: rgbColor(color.themeColor.dark, 0.7),
	  },
	  purchaseStatus: {
	    flex: 1,
	    justifyContent: "flex-start",
	  },
	  divider: {
	    width: "35%",
	    height: 1,
	    backgroundColor: color.line,
	    opacity: 0.5,
	    marginVertical: spacing[3],
	  },
	  info: {
	    flexDirection: "row",
	    alignItems: "center",
	  },
	  coverContainer: {
	    width: COVER_WIDTH,
	    height: COVER_HEIGH,
	    marginRight: spacing[3] + 2,
	  },
	  cover: {
	    width: COVER_WIDTH,
	    aspectRatio: 126 / 173,
	    borderRadius: metrics.borderRadius,
	  },
	  successStatus: {
	    color: color.themeColor.green,
	  },
	  pendingStatus: {
	    color: color.themeColor.yellow,
	  },
	  rejectStatus: {
	    color: color.themeColor.red,
	  },
	})
