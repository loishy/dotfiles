	              preset="semiBoldMP"
	              style={{ color: color.themeColor.green, marginBottom: spacing[1] }}
	            />
	            <Text
	              tx="bookingSuccess.purchaseId"
	              txOptions={{ id: gift?.id }}
	              style={{ color: rgbColor(color.themeColor.dark, 0.7) }}
	            />
	          </View>
	          <ScrollView style={FLEX} showsVerticalScrollIndicator={false}>
