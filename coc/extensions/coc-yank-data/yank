8c26beacd384eba3263bce52340897c8|file:///Users/louis/Dev/Projects/komi/komi-mobile/app/screens/your-sub-interest-screen/your-sub-interest-screen.tsx|147|1|V|typescriptreact
	  const renderCardItem = React.useCallback(
	    ({ item: data }: { item: CategoryModelType[] }) => {
	      return (
	        <View style={[ROW, styles.cardGroupWrapper]}>
	          {data.map((item) => {
	            return (
	              <TouchableOpacity
	                style={MB_4}
	                key={item?.id}
	                onPress={() => _onPressSelect(item?.id)}
	              >
	                <View style={styles.cardWrapper}>
	                  <FastImage source={{ uri: item?.background }} style={styles.image} />
	                  {selectionList.includes(item?.id) ? (
	                    <LinearGradient
	                      colors={[
	                        color.transparent,
	                        rgbColor(color.themeColor.green, 1),
	                        rgbColor(color.themeColor.green, 0.8),
	                      ]}
	                      style={[styles.linearGradient, styles.linearGradientCheck]}
	                      start={{ x: 0, y: 0 }}
	                      locations={[0, 0, 1]}
	                    >
	                      <View style={styles.checkIcon}>
	                        <Icon name="check-vector" />
	                      </View>
	                      <View style={[styles.titleImage, styles.titleImageCheck]}>
	                        <Text
	                          preset="semiBoldR"
	                          style={{ color: color.themeColor.white }}
	                          text={item?.name}
	                        />
	                      </View>
	                    </LinearGradient>
	                  ) : (
	                    <LinearGradient
	                      colors={[
	                        color.transparent,
	                        rgbColor(item?.color, 1),
	                        rgbColor(item?.color, 0.18),
	                      ]}
	                      style={styles.linearGradient}
	                      start={{ x: 0, y: 0 }}
	                      locations={[0, 0, 1]}
	                    >
	                      <View style={styles.titleImage}>
	                        <Text
	                          preset="semiBoldR"
	                          style={{ color: color.themeColor.white }}
	                          text={item?.name}
	                        />
	                      </View>
	                    </LinearGradient>
	                  )}
	                </View>
	              </TouchableOpacity>
	            )
	          })}
	        </View>
	      )
	    },
	    [_onPressSelect, selectionList],
	  )
fb8b55d0a4045689a224e2c71ccd25e8|file:///Users/louis/Dev/Projects/komi/komi-mobile/app/screens/your-sub-interest-screen/your-sub-interest-screen.tsx|130|1|V|typescriptreact
	  const _onPressSelect = React.useCallback(
	    (id) => {
	      if (selectionList.includes(id)) {
	        if (selectionList?.length === REQUIRED_ITEMS) setCanContinue(false)
	        setSelectionList(
	          remove(selectionList, (selectId: string) => {
	            return selectId !== id
	          }),
	        )
	      } else {
	        if (selectionList?.length === REQUIRED_ITEMS - 1) setCanContinue(true)
	        setSelectionList([...selectionList, id])
	      }
	    },
	    [selectionList],
	  )
	
f7effef35327793703c329696a352800|file:///Users/louis/Dev/Projects/komi/komi-mobile/app/screens/your-sub-interest-screen/your-sub-interest-screen.tsx|115|1|V|typescriptreact
	  const groupDataByRow = React.useCallback((numberOfRow: number, data: Array<any>) => {
	    const result = {}
	
	    data.map((item, index) => {
	      const position = Math.floor(index / numberOfRow)
	
	      if (!result[position]) result[position] = [item]
	      else result[position].push(item)
	    })
	
	    return Object.keys(result).map((key) => {
	      return result[key]
	    })
	  }, [])
	
