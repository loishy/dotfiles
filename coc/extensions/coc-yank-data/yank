	        <Text
	          style={[CENTER, MT_2, PX_6]}
	          tx="registerScreen.yourSubInterestDescription"
	          preset="regularR"
	        />
	        <Observer>{() => <View style={styles.tagsContainer}>{renderTags()}</View>}</Observer>
	      </View>
	      <View style={styles.bottomContainer}>
	        <Button
	          loading={loading}
	          onPress={_onHandleSubmit}
	          tx="common.continue"
	          disabled={!canContinue}
	        />
	      </View>
	    </Screen>
	  )
	})
	
