	                    value={values.phone}
	                    maxLength={15}
	                    style={MB_4}
	                    keyboardType="phone-pad"
	                    placeholderTx="registerScreen.phoneNumber"
	                    onChangeText={(value) => {
	                      const testNumber = new AsYouType(
	                        selectCountry?.abbreviation || ("GB" as any),
	                      ).input(selectCountry?.abbreviation === "GB" ? `+44 ${value}` : value)
	
	                      setFieldValue(
	                        "phone",
	                        selectCountry?.abbreviation === "GB"
	                          ? testNumber === "+44"
	                            ? ""
	                            : testNumber.replace("+44 ", "")
	                          : testNumber,
	                      )
	                    }}
