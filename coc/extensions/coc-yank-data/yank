99682853dd4aa093555e007ff7d299ff|file:///Users/louis/Dev/Projects/komi/komi-mobile/app/screens/your-interest-screen/your-interest-screen.tsx|1|1|V|typescriptreact
	import { ParamListBase, useNavigation } from "@react-navigation/native"
	import { toJS } from "mobx"
	import { Observer, observer } from "mobx-react-lite"
	import { CategoryModelType } from "models/model-types"
	import * as React from "react"
	import LinearGradient from "react-native-linear-gradient"
	import { FlatList, StyleSheet, TouchableOpacity, View } from "react-native"
	import {
	  Button,
	  FastImage,
	  ProgressBar,
	  Screen,
	  Text,
	  Icon,
	  resetProgressBar,
	} from "../../components"
	import { useStores } from "../../models"
	import { remove } from "utils/lodash"
	import {
	  color,
	  FLEX,
	  metrics,
	  PX_5,
	  spacing,
	  CENTER,
	  MT_4,
	  MT_2,
	  PX_6,
	  MT_6,
	  MB_4,
	  ROW,
	  MT_8,
	} from "../../theme"
	import { rgbColor } from "utils/color"
	import { NativeStackNavigationProp } from "react-native-screens/lib/typescript"
	import { NavigationRoutes } from "navigation/navigation-routes"
	
	const NUMBER_OF_ROWS = 2
	const REQUIRED_ITEMS = 3
	
	interface YourInterestScreenProps {
	  navigation: NativeStackNavigationProp<ParamListBase>
	}
	
	export const YourInterestScreen = observer((props: YourInterestScreenProps) => {
	  const {
	    authStore: { registerStep, totalRegisterStep, setRegisterStep, updateConsumerProfile },
	    categoryStore: { list, getCategoryList },
	  } = useStores()
	  const [loading, setLoading] = React.useState<boolean>(false)
	  const [selectionList, setSelectionList] = React.useState([])
	  const [canContinue, setCanContinue] = React.useState(false)
	
	  const navigation = useNavigation()
	
	  const _goBack = React.useCallback(() => {
	    navigation.goBack()
	  }, [])
	
	  React.useEffect(() => {
	    getCategoryList({ page: 1 })
	  }, [])
	
	  const _onHandleSubmit = React.useCallback(async () => {
	    setLoading(true)
	    const result = await updateConsumerProfile({ categories: selectionList })
	    setLoading(false)
	
	    if (result) {
	      setRegisterStep(1)
	      resetProgressBar()
	
	      props.navigation.reset({
	        index: 0,
	        routes: [{ name: NavigationRoutes.MainStack }],
	      })
	    }
	  }, [selectionList])
	
	  const groupDataByRow = React.useCallback((numberOfRow: number, data: Array<any>) => {
	    const result = {}
	
	    data.map((item, index) => {
	      const position = Math.floor(index / numberOfRow)
	
	      if (!result[position]) result[position] = [item]
	      else result[position].push(item)
	    })
	
	    return Object.keys(result).map((key) => {
	      return result[key]
	    })
	  }, [])
	
	  const _onPressSelect = React.useCallback(
	    (id) => {
	      if (selectionList.includes(id)) {
	        if (selectionList?.length === REQUIRED_ITEMS) setCanContinue(false)
	        setSelectionList(
	          remove(selectionList, (selectId: string) => {
	            return selectId !== id
	          }),
	        )
	      } else {
	        if (selectionList?.length === REQUIRED_ITEMS - 1) setCanContinue(true)
	        setSelectionList([...selectionList, id])
	      }
	    },
	    [selectionList],
	  )
	
	  const renderCardItem = React.useCallback(
	    ({ item: data }: { item: CategoryModelType[] }) => {
	      return (
	        <View style={[ROW, styles.cardGroupWrapper]}>
	          {data.map((item) => {
	            return (
	              <TouchableOpacity
	                style={MB_4}
	                key={item?.id}
	                onPress={() => _onPressSelect(item?.id)}
	              >
	                <View style={styles.cardWrapper}>
	                  <FastImage source={{ uri: item?.background }} style={styles.image} />
	                  {selectionList.includes(item?.id) ? (
	                    <LinearGradient
	                      colors={[
	                        color.transparent,
	                        rgbColor(color.themeColor.green, 1),
	                        rgbColor(color.themeColor.green, 0.8),
	                      ]}
	                      style={[styles.linearGradient, styles.linearGradientCheck]}
	                      start={{ x: 0, y: 0 }}
	                      locations={[0, 0, 1]}
	                    >
	                      <View style={styles.checkIcon}>
	                        <Icon name="check-vector" />
	                      </View>
	                      <View style={[styles.titleImage, styles.titleImageCheck]}>
	                        <Text
	                          preset="semiBoldR"
	                          style={{ color: color.themeColor.white }}
	                          text={item?.name}
	                        />
	                      </View>
	                    </LinearGradient>
	                  ) : (
	                    <LinearGradient
	                      colors={[
	                        color.transparent,
	                        rgbColor(item?.color, 1),
	                        rgbColor(item?.color, 0.18),
	                      ]}
	                      style={styles.linearGradient}
	                      start={{ x: 0, y: 0 }}
	                      locations={[0, 0, 1]}
	                    >
	                      <View style={styles.titleImage}>
	                        <Text
	                          preset="semiBoldR"
	                          style={{ color: color.themeColor.white }}
	                          text={item?.name}
	                        />
	                      </View>
	                    </LinearGradient>
	                  )}
	                </View>
	              </TouchableOpacity>
	            )
	          })}
	        </View>
	      )
	    },
	    [_onPressSelect, selectionList],
	  )
	
	  return (
	    <Screen
	      header={{
	        leftIcon: "sLeft",
	        onLeftPress: _goBack,
	        style: styles.header,
	        centerComponent: (
	          <ProgressBar style={styles.progress} total={totalRegisterStep} step={registerStep} />
	        ),
	        rightContainerStyle: FLEX,
	        leftContainerStyle: FLEX,
	        centerContainerStyle: styles.centerHeader,
	      }}
	      preset="scroll"
	    >
	      <View style={PX_5}>
	        <Text style={[CENTER, MT_4]} tx="registerScreen.yourInterest" preset="semiBoldMP" />
	        <Text
	          style={[CENTER, MT_2, PX_6]}
	          tx="registerScreen.yourInterestDescription"
	          preset="regularR"
	        />
	        <Observer>
	          {() => (
	            <FlatList
	              keyExtractor={(_item: any, index: number) => `${index}`}
	              style={MT_6}
	              data={groupDataByRow(NUMBER_OF_ROWS, toJS(list))}
	              renderItem={renderCardItem}
	            />
	          )}
	        </Observer>
	        <Button
	          loading={loading}
	          onPress={_onHandleSubmit}
	          tx="common.continue"
	          containerStyle={MT_8}
	          disabled={!canContinue}
	        />
	      </View>
	    </Screen>
	  )
	})
	
	const styles = StyleSheet.create({
	  header: {
	    borderBottomWidth: 0,
	  },
	
	  progress: {
	    width: "100%",
	    marginLeft: spacing[6],
	  },
	
	  centerHeader: {
	    flex: 6,
	  },
	
	  cardGroupWrapper: {
	    justifyContent: "space-between",
	  },
	  cardWrapper: {
	    width: metrics.screenWidth * 0.413,
	    height: metrics.screenHeight * 0.088,
	  },
	  image: {
	    width: metrics.screenWidth * 0.413,
	    height: metrics.screenHeight * 0.088,
	    borderRadius: metrics.borderRadius,
	  },
	  titleImage: {
	    width: "70%",
	    paddingLeft: spacing[3],
	  },
	  titleImageCheck: {
	    opacity: 0.3,
	  },
	  linearGradient: {
	    width: "100%",
	    height: "100%",
	    justifyContent: "center",
	    position: "absolute",
	    borderRadius: metrics.borderRadius,
	  },
	  linearGradientCheck: {
	    zIndex: 2,
	  },
	  checkIcon: {
	    position: "absolute",
	    width: "100%",
	    justifyContent: "center",
	    alignItems: "center",
	  },
	})
