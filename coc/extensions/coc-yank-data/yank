	  .actions((self) => ({
	    removePaymentMethod: flow(function* (paymentToken: string, isRefresh: boolean) {
	      const result = yield self.environment.api.deletePaymentMethod(paymentToken)
	      if (result.kind === "ok") {
	        const newData: any = self.savedMethods.filter((el) => el.token !== paymentToken)
	
	        if (!!newData.length && isRefresh) {
	          const result = yield self.environment.api.updateDefaultPaymentMethod(newData[0].token)
	          result.kind === "ok" && (newData[0].default = true)
	          self.setSelectedPayment(0)
	        }
	        self.savedMethods = newData
	      }
	      notifications.showApiError(result)
	    }),
	  }))
